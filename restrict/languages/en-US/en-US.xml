<?xml version="1.0" encoding="ISO-8859-1"?>
<mindLanguage>
	<verbs> <!-- possessive verbs, or verbs that indicate something linked to another thing -->
		<verb value="has"/>
		<verb value="have"/>
		<verb value="contains"/>
		<verb value="contain"/>
		<verb value="adds"/>
		<verb value="add"/>
		<verb value="counts with"/>
		<verb value="count with"/>
	<verb value="teaches"/></verbs>
	<belongs> <!-- possibly possessive verbs. Something that might belong another thing -->
		<verb value="may have"/>
		<verb value="may be"/>
		<verb value="might have"/>
		<verb value="might be"/>
		<verb value="can have"/>
		<verb value="can be"/>
		<verb value="maybe has"/>
		<verb value="maybe have"/>
		<verb value="belongs to"/>
	</belongs>
	<obligation> <!-- mandatory expressions. When something is or must/should or will be another thing -->
		<verb value="is"/>
		<verb value="are"/>
		<verb value="will be"/>
		<verb value="are gonna be"/>
		<verb value="are goning to be"/>
		<verb value="is going to be"/>
		<verb value="is gonna be"/>
		<verb value="ought to be"/>
		<verb value="should be"/>
		<verb value="must be"/>
	</obligation>
	<types> <!-- how to recognize these types, which words could be used -->
		<type value="char">
			<substantive value="character"/>
		    <substantive value="char"/>
		    <substantive value="characters"/>
		    <substantive value="letters"/>
		    <substantive value="letter"/>
		    <substantive value="alpha"/>
		</type>
		<type value="varchar">
			<substantive value="varchar"/>
		    <substantive value="character varying"/>
		    <substantive value="variant character"/>
		    <substantive value="variant character"/>
		    <substantive value="field"/>
		    <substantive value="input"/>
		    <substantive value="string"/>
		    <substantive value="description"/>
		    <substantive value="detail"/>
		</type>
		<type value="text">
		    <substantive value="free text"/>
			<substantive value="text"/>
		    <substantive value="content"/>
			<substantive value="textarea"/>
		</type>
		<type value="password">
			<substantive value="password"/>
		    <substantive value="pwd"/>
		    <substantive value="keyword"/>
		    <substantive value="access word"/>
		    <substantive value="access"/>
		    <substantive value="key"/>
		</type>
		<type value="file">
			<substantive value="file"/>
		    <substantive value="blob"/>
		    <substantive value="binary"/>
		    <substantive value="lo"/>
		    <substantive value="image"/>
		    <substantive value="grafic"/>
		    <substantive value="chart"/>
		    <substantive value="doc"/>
		    <substantive value="pdf"/>
		    <substantive value="document"/>
		    <substantive value="spreedsheet"/>
		</type>
		<type value="smallint">
			<substantive value="smallint"/>
			<substantive value="tiny int"/>
		</type>
		<type value="int">
			<substantive value="int"/>
		    <substantive value="integer"/>
		    <substantive value="numeric"/>
		    <substantive value="number"/>
		</type>
		<type value="bigint">
			<substantive value="bigint"/>
		</type>
		<type value="real">
			<substantive value="float"/>
			<substantive value="real"/>
			<substantive value="floating"/>
			<substantive value="value"/>
			<substantive value="money"/>
			<substantive value="percent"/>
			<substantive value="fraction"/>
			<substantive value="double"/>
		<substantive value="coins"/></type>
		<type value="time">
			<substantive value="date\/time"/>
			<substantive value="time\/date"/>
			<substantive value="date and time"/>
			<substantive value="time and date"/>
			<substantive value="datetime"/>
			<substantive value="date time"/>
			<substantive value="date"/>
			<substantive value="time"/>
			<substantive value="clock"/>
			<substantive value="calendar"/>
		</type>
	</types>
	<required> <!-- keywords that specify when some property is not null -->
		<adjective value="obly"/>
		<adjective value="obligatory"/>
		<adjective value="required"/>
		<adjective value="mandatory"/>
		<adjective value="not null"/>
		<adjective value="needed"/>
	</required>
	<unique> <!-- keywords that specify when some property is must be unique -->
		<adjective value="unique"/>
		<adjective value="distinct"/>
		<adjective value="key"/>
	</unique>
	<especialchars> <!-- special chars that can be used, but will not be used into the database -->
