	<quantifiersId> <!-- indicate when something can has the minimun and maximun cardinality -->
		<world value="or"/>
		<world value="up to"/>
		<world value="untill"/>
		<world value="till"/>
		<world value="to"/>
	</quantifiersId>
	<quantifiers> <!-- possible quantifiers -->
		<min> <!-- the minimun -->
			<min>  <!-- when 0 -->
				<quantifier value="0"/>
				<quantifier value="zero"/>
			    <quantifier value="none"/>
			    <quantifier value="noone"/>
			    <quantifier value="no one"/>
			    <quantifier value="no"/>
			    <quantifier value="not"/>
			    <quantifier value="not even"/>
			</min>
			<max> <!-- when 1 -->
				<quantifier value="1"/>
				<quantifier value="one"/>
				<quantifier value="once"/>
				<quantifier value="only one"/>
				<quantifier value="the"/>
				<quantifier value="an"/>
				<quantifier value="a"/>
			</max>
		</min>
		<max> <!-- the maximun -->
			<min> <!-- when 1 -->
				<quantifier value="one"/>
				<quantifier value="onece"/>
				<quantifier value="1"/>
				<quantifier value="only one"/>
				<quantifier value="a"/>
			</min>
			<max> <!-- when many -->
				<quantifier value="n"/>
				<quantifier value="m"/>
				<quantifier value="x"/>
				<quantifier value="many"/>
				<quantifier value="more of"/>
				<quantifier value="more"/>
				<quantifier value="too many"/>
				<quantifier value="a lot of"/>
				<quantifier value="a lot"/>
				<quantifier value="lots of"/>
				<quantifier value="lots"/>
				<quantifier value="a bunch of"/>
				<quantifier value="several of"/>
				<quantifier value="several"/>
				<quantifier value="a few of"/>
				<quantifier value="a few"/>
				<quantifier value="few of"/>
				<quantifier value="few"/>
				<quantifier value="some of"/>
				<quantifier value="some"/>
			</max>
		</max>
	</quantifiers>
</mindLanguage>
