@charset "UTF-8";

HTML
{
	overflow: hidden;
}
body
{
	margin:0px;padding:0px;
	background-color: #fff;
	color: #666;
	font-family: Tahoma, Arial, Sans-Serif;
	font-size: 11px;
	cursor:crosshair;
}
div, td, span
{
	cursor:default;
}
select
{
	background-color:#fff;
}
body .style a
{
	color:gray;
	font-family:sans-serif;
	font-size:13px;
	text-decoration:none;
}
FORM
{
	margin:0px;
	padding:0px;
}
input[type=text]
{
	height:23px;
	background-color:#fff;
}
pre
{
	margin:0px;
}
