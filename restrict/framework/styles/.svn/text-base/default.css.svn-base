}

.mind_dialog{
	display:none;
}
.userAdd
{
	width:100%;
	height: 100%;
	background-image:url(../../images/background/aser_add.gif);
	background-position:bottom right;
	background-repeat: no-repeat;
}
.feedBack td
{
	font-size: 11px;
}
.feedBack
{
	width:100%;
	height: 100%;
	background-image:url(../../images/background/speak.png);
	background-position:top center;
	background-repeat: no-repeat;
}
.config
{
	width:100%;
	height: 100%;
	background-image:url(../../images/background/config.gif);
	background-position:bottom right;
	background-repeat: no-repeat;
}
.errorMessageLabel
{
	border: solid 1px #000;
	background-color: #f00;
	color:#fff;
	font-weight: bold;
	text-align: justify;
}

.mind-message{
	display:none;
}

.mind-message-error{
	padding:2px;
	margin-bottom:4px;
	border:solid 1px #b9b9b9;
	background-color:#fc5f5f;
}

.mind-message-alert{
	padding:2px;
	margin-bottom:4px;
	border:solid 1px #b9b9b9;
	background-color: #fbf58e;
}

.mind-alert{
	display:none;
}

.mind-data{
	display:none;
};

.button-message{
	cursor:pointer;
	margin-right:10px;		
	float:left;
	width:25px;
	height:25px;
	background-image: url(../../images/close.gif);
	background-repeat:no-repeat;
}

.manageList{
	float:left;
	width: 180px;
	height: 95%;
	font-weight: bold;
	color: gray;
	padding: 5px;
	border: solid 1px gray;
}
.manageListContent{	
	font-family:arial;
	font-size:15px;
	cursor:pointer;
}
.manageListContentSelected{
	background-color:gray;
	color:white;
}

.manageListContentOpened{
	display:none;	
}

.manageContent{	
	width: 100%;
	height: 95%;
	font-weight: bold;
	color: red;
	padding: 5px;
	border: solid 1px gray;
}
