	padding-bottom:1px;
}

.acceleratorsMessageBox
{
	color: #000;
	background-color: #f2f2f2;
	border: solid 1px #666;
	padding: 6px;
	font-size: 14px;
}
.acceleratorsShortCutsBox
{
	color: #000;
	background-color: #f2f2f2;
	border: solid 1px #666;
	padding: 30px;
	font-size: 14px;
}
.appBody
{
	background-color: #fff;
	height: 100%;
	width: 100%;
	padding: 0px;
	margin: 0px;
}
.nothing
{
	background-color: #eee;
	height:100%;
}

.iframe{
	width:100%;
	height:100%;
